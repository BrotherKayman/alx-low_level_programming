#include <stdio.h>
#include "main.h"
/**
 * _isdigit -  function that checks for a digit
 * @c: holds integer
 * Return: 1 if digit, otherwise 0
 */
int _isdigit(int c)
{
if (c >= '0' && c <= '9')
{
return (1);
}
return (0);
}
/**
* main - Entry Point
* Return: 0 on success
*/
int main(void)
{
int _isdigit(int c);
return (0);
}
