#include "main.h"
/**
 * main - Entry Point
 *
 * @n: check number
 * print_sign: - prints the sign of a number
 *
 * Return: 0 Always
 */
int print_sign(int n)
{
if (n > 0)
{
_putchar('+');
return (1);
}
else if (n == 0)
{
_putchar('0');
return (0);
}
return (0);
}
int main(void)
{
int print_sign(int n);
return (0);
}
