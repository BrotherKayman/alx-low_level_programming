#include "main.h"
#include <stdlib.h>
#include <string.h>
char *_strdup(char *s)
{
    char *duplicate;
    size_t x;
    x = strlen(s) + 1;
  if (s == NULL)
    {
    return (NULL);
  }

    duplicate = (char *)malloc(x);
  if (duplicate != NULL)
    {
    strcpy(duplicate, s);
  }
  return (duplicate);
}
