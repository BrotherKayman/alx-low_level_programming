#include <stdio.h>
#include "main.h"
/**
* _islower - Checks if a character is lowercase.
* @c: The character to be checked.
*
* Return: 1 if @c is lowercase, 0 otherwise.
*/
int _islower(int c)
{
return (c >= 'a' && c <= 'z');
}
/**
* main - Entry Point
* Return: 0 at Success
*/
int main(void)
{
int _islower(int c);
printf("Test this");
return (0);
}
