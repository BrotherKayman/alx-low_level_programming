#include "main.h"
#include <stdio.h>
/**
 * string_toupper - Function changes all lowercase letters of a string to uppercase.
 */
char *string_toupper(char *n)

{

  while (n)
   *n++;
    return (n);
    }
int main(void)
{
  char str[] = "Look up!\n";
  char *ptr;

  ptr = string_toupper(str);
  printf("%s", ptr);
  printf("%s", str);
  return (0);
}
